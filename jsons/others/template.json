{
  "topic": "Category",
  "subcategory_of": "Parent Category", // can be left out if it's a main category (parent category doesnt need to use the exact topic name)
  "location": "fun/drops", // default is lowercase of `topic`
  "packs": [
    {
      "pack_id": "pack_name with no space", // required
      "pack_name": "name of pack", // required
      "pack_description": "description of pack", // required
      "conflict": ["packid of conflicting pack"], // can be unset
      "message": ["warn (red)/error (orange)/info (cyan)", "message for it"], // can be unset
      "icon": "extension of pack icon like gif or jpg (no '.')", // default is png, can be unset
      "priority": "priority of pack as an integer", // default is 0, can be unset
      "regolith": "true" // if pack is regolith, can be unset
    }
  ]
}
